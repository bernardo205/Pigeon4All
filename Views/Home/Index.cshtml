<script>

    window.addEventListener('scroll', function () {
        const scrollTop = window.scrollY;
        if (scrollTop >= 99) {
            header.classList.add('scrolled');
            img.src = '/img/logotipo/logo_pigeon4all_horiz_original.png'
        }
        else {
            header.classList.remove('scrolled');
            img.src = '/img/logotipo/logo_pigeon4all_horiz_branco.png'
        }


    });


    const items = document.querySelectorAll('.carousel-item');
    const indicators = document.querySelectorAll('.indicator');
    const currentSlideIndicator = document.getElementById('current-slide');
    const totalSlidesIndicator = document.getElementById('total-slides');
    let currentItem = 0;
    const intervalTime = 30000; // 30 segundos

    totalSlidesIndicator.textContent = items.length.toString().padStart(2, '0');

    function showItem(index) {
        items[currentItem].classList.remove('active');
        indicators[currentItem].classList.remove('active');
        currentItem = index;
        items[currentItem].classList.add('active');
        indicators[currentItem].classList.add('active');
        currentSlideIndicator.textContent = (currentItem + 1).toString().padStart(2, '0');
    }

    function showNextItem() {
        let nextItem = (currentItem + 1) % items.length;
        showItem(nextItem);
    }

    indicators.forEach((indicator, index) => {
        indicator.addEventListener('click', () => {
            showItem(index);
        });
    });

    setInterval(showNextItem, intervalTime);


    const endDate = new Date("2024-12-31T23:59:59Z");

    function updateCountdown() {
        const now = new Date();
        const diff = endDate - now;

        if (diff <= 0) {
            document.getElementById('days').textContent = '00';
            document.getElementById('hours').textContent = '00';
            document.getElementById('minutes').textContent = '00';
            document.getElementById('seconds').textContent = '00';
            return;
        }

        const days = Math.floor(diff / (1000 * 60 * 60 * 24));
        const hours = Math.floor((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        const minutes = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));
        const seconds = Math.floor((diff % (1000 * 60)) / 1000);

        document.getElementById('days').textContent = String(days).padStart(2, '0');
        document.getElementById('hours').textContent = String(hours).padStart(2, '0');
        document.getElementById('minutes').textContent = String(minutes).padStart(2, '0');
        document.getElementById('seconds').textContent = String(seconds).padStart(2, '0');
    }

    setInterval(updateCountdown, 1000);
    updateCountdown();




    const cardSlider = document.querySelector('.card-slider');
    const cards = document.querySelectorAll('.d-card');
    const prevButton = document.querySelector('#prev');
    const nextButton = document.querySelector('#next');

    let currentIndex = 0;

    prevButton.addEventListener('click', () => {
        currentIndex--;
        if (currentIndex < 0) {
            currentIndex = cards.length - 1;
        }
        updateSlider();
    });

    nextButton.addEventListener('click', () => {
        currentIndex++;
        if (currentIndex >= cards.length) {
            currentIndex = 0;
        }
        updateSlider();
    });

    function updateSlider() {
        cards.forEach((card, index) => {
            card.classList.remove('activ');
            if (index === currentIndex) {
                card.classList.add('activ');
            }
        });
        cardSlider.style.transform = `translateX(${currentIndex * -340}px)`; /* move o slider para o cartão selecionado */
    }
</script>