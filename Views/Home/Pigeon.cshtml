<script>

    document.addEventListener('DOMContentLoaded', function () {
        header.classList.add('scrolled');
        header.classList.remove('fixed-top');
        img.src = '/img/logotipo/logo_pigeon4all_horiz_original.png'
    });


    var acc = document.getElementsByClassName("accordion");
    var i;

    for (i = 0; i < acc.length; i++) {
        acc[i].addEventListener("click", function () {
            this.classList.toggle("active");
            var panel = this.nextElementSibling;
            if (panel.style.maxHeight) {
                panel.style.maxHeight = null;
            } else {
                panel.style.maxHeight = panel.scrollHeight + 10 + "px";
            }
        });
    }


    const endDate = new Date("2024-06-24T17:00:00Z");

    function updateCountdown() {
        const now = new Date();
        const diff = endDate - now;

        if (diff <= 0) {
            document.getElementById('days').textContent = '00';
            document.getElementById('hours').textContent = '00';
            document.getElementById('minutes').textContent = '00';
            document.getElementById('seconds').textContent = '00';
            return;
        }

        const days = Math.floor(diff / (1000 * 60 * 60 * 24));
        const hours = Math.floor((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        const minutes = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));
        const seconds = Math.floor((diff % (1000 * 60)) / 1000);

        document.getElementById('days').textContent = String(days).padStart(2, '0') + 'd';
        document.getElementById('hours').textContent = String(hours).padStart(2, '0') + 'h';
        document.getElementById('minutes').textContent = String(minutes).padStart(2, '0') + 'm';
        document.getElementById('seconds').textContent = String(seconds).padStart(2, '0') + 's';
    }

    setInterval(updateCountdown, 1000);
    updateCountdown();



    const images = document.querySelectorAll('.carousel-image');
    const track = document.querySelector('.carousel-track');
    const prevButton = document.getElementById('prevButton');
    const nextButton = document.getElementById('nextButton');
    const currentImageElement = document.getElementById('currentImage');
    const totalImagesElement = document.getElementById('totalImages');

    const imagesPerPage = 5;
    let currentIndex = 0;

    function updateCarousel() {
        const offset = currentIndex * -100; // Ajuste de acordo com a largura da imagem
        track.style.transform = `translateX(${offset}px)`;

        currentImageElement.textContent = currentIndex + 1;
        prevButton.disabled = currentIndex === 0;
        nextButton.disabled = currentIndex >= images.length - imagesPerPage;
    }

    function showNextImages() {
        if (currentIndex < images.length - imagesPerPage) {
            currentIndex++;
            updateCarousel();
        }
    }

    function showPrevImages() {
        if (currentIndex > 0) {
            currentIndex--;
            updateCarousel();
        }
    }

    prevButton.addEventListener('click', showPrevImages);
    nextButton.addEventListener('click', showNextImages);


    totalImagesElement.textContent = images.length;
    updateCarousel();

    const imageContainer = document.getElementById('image-container');
    const imageElements = imageContainer.querySelectorAll('img');

    const replacementImageURLs = {
        'image1': '/img/Imagens/12f3dd61-ba8f-4d78-8082-8971cedec893 - Cópia.jpg',
        'image2': '/img/Imagens/ae3a71c2-9ea2-4338-863b-c3eab7c9b753 - Cópia.jpg',
        'image3': '/img/Imagens/12f3dd61-ba8f-4d78-8082-8971cedec893 - Cópia.jpg',
        'image4': '/img/Imagens/ae3a71c2-9ea2-4338-863b-c3eab7c9b753 - Cópia.jpg',
        'image5': '/img/Imagens/12f3dd61-ba8f-4d78-8082-8971cedec893 - Cópia.jpg',
        'image6': '/img/Imagens/ae3a71c2-9ea2-4338-863b-c3eab7c9b753 - Cópia.jpg',
        'image7': '/img/Imagens/12f3dd61-ba8f-4d78-8082-8971cedec893 - Cópia.jpg',
        'image8': '/img/Imagens/ae3a71c2-9ea2-4338-863b-c3eab7c9b753 - Cópia.jpg',
        'image9': '/img/Imagens/12f3dd61-ba8f-4d78-8082-8971cedec893 - Cópia.jpg',
        'image10': '/img/Imagens/ae3a71c2-9ea2-4338-863b-c3eab7c9b753 - Cópia.jpg'
    };

    const targetImage = document.getElementById('targetImage');

    for (const image of imageElements) {
        image.addEventListener('click', function (event) {
            const clickedImage = event.currentTarget;
            const imageId = clickedImage.id;    

            // Remove any existing class (optional)
            clickedImage.classList.remove('previous-class'); // Replace 'previous-class' with your previous class name (if any)

            // Add unique class based on ID
            clickedImage.classList.add(imageId); // Add the image's ID as the class name

            console.log('Clicked image:', imageId); // Log the ID of the clicked image

            // Check if the clicked image ID has a corresponding replacement URL
            const replacementURL = replacementImageURLs[imageId];
            if (replacementURL) {
                targetImage.src = replacementURL; // Update the target image's src
            } else {
                console.warn(`No replacement URL found for image ID: ${imageId}`);
            }
        });
    }

